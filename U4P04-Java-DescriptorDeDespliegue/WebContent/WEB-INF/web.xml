<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>U4P04-Java-DescriptorDeDespliegue</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>indice.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/errores/error404.html</location>
  </error-page>
  <servlet>
    <servlet-name>servletDeEjemploWeb</servlet-name>
    <servlet-class>servlets.EjemploServletHttp</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletDeEjemploWeb</servlet-name>
    <url-pattern>/EjemploServletHttp</url-pattern>
    <url-pattern>/SampleHttpServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EjemploServlet</servlet-name>
    <servlet-class>servlets.EjemploServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>EjemploServlet</servlet-name>
    <url-pattern>/EjemploServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EjemploServletGenerico</servlet-name>
    <servlet-class>servlets.EjemploServletGenerico</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>EjemploServletGenerico</servlet-name>
    <url-pattern>/EjemploServletGenerico</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FechaActual</servlet-name>
    <servlet-class>EjemploAplicacion.FechaActual</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>FechaActual</servlet-name>
    <url-pattern>/FechaActual</url-pattern>
    <url-pattern>/ShowDate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NumeroAleatorio</servlet-name>
    <servlet-class>EjemploAplicacion.NumeroAleatorio</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>NumeroAleatorio</servlet-name>
    <url-pattern>/NumeroAleatorio</url-pattern>
    <url-pattern>/RandomNumber</url-pattern>
  </servlet-mapping>
</web-app>